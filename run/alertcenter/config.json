{
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "./alertcenter",
        "flag": "fatal,error,warn,info,debug"
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:4444",
            "udp@0.0.0.0:4444"
        ],
        "max_pkg_size": 10240
    },
    "redis": {
        "name": "redis-cluster",
        "addr": [
            "127.0.0.1:16380"
            ]        
    }
}
